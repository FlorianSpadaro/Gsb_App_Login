<?xml version="1.0" encoding="utf-8"?>

    <TabHost xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:id="@+id/TabHostPract">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TabWidget
                android:id="@android:id/tabs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <FrameLayout
                android:id="@android:id/tabcontent"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/tab1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:weightSum="1">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="0.09">

                        <TextView
                            android:text="Nom"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView5"
                            android:textSize="18sp"
                            android:layout_below="@+id/autoCompleteTextPract"
                            android:layout_marginTop="16dp"
                            android:textStyle="normal|bold"
                            android:layout_alignRight="@+id/textView7"
                            android:layout_alignEnd="@+id/textView7" />

                        <TextView
                            android:text="PrÃ©nom"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView6"
                            android:textSize="18sp"
                            android:layout_marginTop="12dp"
                            android:layout_below="@+id/textView5"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:textStyle="normal|bold" />

                        <TextView
                            android:text="Telephone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView9"
                            android:textSize="18sp"
                            android:layout_marginTop="12dp"
                            android:layout_below="@+id/textView8"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:textStyle="normal|bold" />

                        <TextView
                            android:text="Coeff"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView8"
                            android:textSize="18sp"
                            android:layout_marginTop="12dp"
                            android:layout_below="@+id/textView7"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:textStyle="normal|bold" />

                        <TextView
                            android:text="Type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView7"
                            android:textSize="18sp"
                            android:layout_marginTop="12dp"
                            android:layout_below="@+id/textView6"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:textStyle="normal|bold" />

                        <TextView
                            android:text="Code Postal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView3"
                            android:textSize="18sp"
                            android:layout_marginTop="12dp"
                            android:layout_below="@+id/textView9"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:textStyle="normal|bold" />

                        <TextView
                            android:text="Adresse"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView4"
                            android:textSize="18sp"
                            android:textStyle="normal|bold"
                            android:layout_below="@+id/textView3"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_marginTop="12dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textViewMedNom"
                            android:textSize="18sp"
                            android:layout_above="@+id/textView6"
                            android:layout_toRightOf="@+id/textView3"
                            android:layout_toEndOf="@+id/textView3"
                            android:layout_marginLeft="21dp"
                            android:layout_marginStart="21dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textViewVisiteurAdresse"
                            android:textSize="18sp"
                            android:layout_alignBaseline="@+id/textView4"
                            android:layout_alignBottom="@+id/textView4"
                            android:layout_alignLeft="@+id/textViewMedNom"
                            android:layout_alignStart="@+id/textViewMedNom" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textViewPrenom"
                            android:textSize="18sp"
                            android:layout_alignBaseline="@+id/textView6"
                            android:layout_alignBottom="@+id/textView6"
                            android:layout_alignLeft="@+id/textViewMedNom"
                            android:layout_alignStart="@+id/textViewMedNom" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textViewType"
                            android:textSize="18sp"
                            android:layout_above="@+id/textView8"
                            android:layout_alignLeft="@+id/textViewPrenom"
                            android:layout_alignStart="@+id/textViewPrenom" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textViewTel"
                            android:textSize="18sp"
                            android:layout_alignBaseline="@+id/textView9"
                            android:layout_alignBottom="@+id/textView9"
                            android:layout_alignLeft="@+id/textViewVisiteurAdresse"
                            android:layout_alignStart="@+id/textViewVisiteurAdresse" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textViewCP"
                            android:textSize="18sp"
                            android:layout_alignBaseline="@+id/textView3"
                            android:layout_alignBottom="@+id/textView3"
                            android:layout_alignLeft="@+id/textViewVisiteurAdresse"
                            android:layout_alignStart="@+id/textViewVisiteurAdresse" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textViewCoeff"
                            android:textSize="18sp"
                            android:layout_above="@+id/textView9"
                            android:layout_alignLeft="@+id/textViewCP" />

                        <AutoCompleteTextView
                            android:layout_height="wrap_content"
                            android:id="@+id/autoCompleteTextPract"
                            android:textAlignment="center"
                            android:contentDescription="Rechercher un practicien"
                            android:layout_width="match_parent"
                            android:layout_alignBaseline="@+id/buttonPractOk"
                            android:layout_alignBottom="@+id/buttonPractOk"
                            android:layout_toLeftOf="@+id/buttonPractOk"
                            android:layout_toStartOf="@+id/buttonPractOk"
                            android:hint="Practicien"/>

                        <Button
                            android:text="Ok"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/buttonPractOk"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentEnd="true" />

                    </RelativeLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/tab2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:text="Something is comming"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView25"
                            android:textAlignment="center"
                            android:layout_centerVertical="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true" />
                    </RelativeLayout>

                </LinearLayout>

            </FrameLayout>
        </LinearLayout>
    </TabHost>